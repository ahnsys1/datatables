{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "name": "Launch add-on",
            "addonPath": "${workspaceFolder}"
        },
        {
            "name": "Launch Edge2",
            "request": "launch",
            "type": "msedge",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Launch Edge",
            "request": "launch",
            "type": "msedge",
            "sourceMaps": true,
            "url": "http://localhost:4200",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {

            "type": "java",
            "name": "BackendApplication",
            "request": "launch",
            "mainClass": "com.angular.backend.BackendApplication",
            "projectName": "backend"
        },
        {
            "type": "java",
            "name": "BackendApplicationTests",
            "request": "launch",
            "mainClass": "com.angular.backend.BackendApplicationTests",
            "projectName": "backend"
        },
        {
            "type": "java",
            "name": "Application",
            "request": "launch",
            "mainClass": "com.music.application.Application",
            "projectName": "music"
        }
    ]
}